{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "0.1.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint . --no-error-on-unmatched-pattern",
    "lint:fix": "eslint . --fix --no-error-on-unmatched-pattern",
    "format": "prettier --check \"**/*.{ts,tsx}\" --ignore-path=\"../../.prettierignore\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx}\" --ignore-path=\"../../.prettierignore\"",
    "typecheck": "tsc --noEmit",
    "build:ios": "eas build -p ios --profile production",
    "build:dev": "eas build --profile development --platform ios",
    "build:android": "eas build -p android --profile production",
    "build:web": "eas build -p web --profile production",
    "build:all": "pnpm run build:ios && pnpm run build:android && pnpm run build:web",
    "storybook-generate": "sb-rn-get-stories",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:fix-requires": "node scripts/fix-storybook-requires.js",
    "storybook-generate:fixed": "pnpm storybook-generate && pnpm storybook:fix-requires"
  },
  "prettier": "@studio/prettier-config",
  "jest": {
    "preset": "jest-expo"
  },
